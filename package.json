{
    "name": "goto-documentation",
    "displayName": "vscode-goto-documentation",
    "description": "VS Code extension to go to documentation",
    "version": "0.1.0",
    "publisher": "friedbrice",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/friedbrice/vscode-goto-documentation"
    },
    "bugs": {
        "url": "https://github.com/friedbrice/vscode-goto-documentation/issues"
    },
    "license": "Apache License 2.0",
    "icon": "doc/img/icon.png",
    "activationEvents": [
        "onCommand:extension.gotoDocument"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.gotoDocument",
                "title": "Browse documentation"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Goto Documentation configuration",
            "properties": {
                "goto-documentation.customDocs": {
                    "type": "object",
                    "default": {},
                    "description": "Override or extend mapping of file extentions to URLs. E.g. {\"md\": \"http://en.wikipedia.org/wiki/${query}\"}"
                }
            }
        },
        "keybindings": [
            {
                "command": "extension.gotoDocument",
                "key": "ctrl+shift+h",
                "mac": "cmd+shift+h",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.gotoDocument",
                    "group": "navigation@10"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "open": "^5.4.0"
    }
}
